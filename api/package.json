{
    "name": "@templatenametoreplace/api",
    "config": {
        "product": "templatenametoreplace",
        "component": "api"
    },
    "description": "",
    "version": "1.0.0",
    "scripts": {
        "bootstrap": "./scripts/bootstrap.sh $npm_package_config_product $npm_package_config_component",
        "nuke": "./scripts/nuke.sh $npm_package_config_product $npm_package_config_component",
        "prestart": "npm run build",
        "start": "sam local start-api -p 3001 --env-vars ./environments/localenv.json",
        "test": "jest",
        "clean": "rm -rf deploy",
        "build": "pnpm clean && webpack",
        "deploy:nonprod": "./scripts/deploy.sh $npm_package_config_product $npm_package_config_component nonprod",
        "deploy:prod": "./scripts/deploy.sh $npm_package_config_product $npm_package_config_component prod",
        "e2e:local": "newman run e2e/collection.postman_collection.json -e e2e/nonprod.postman_environment.json --bail",
        "e2e:nonprod": "newman run e2e/collection.postman_collection.json -e e2e/nonprod.postman_environment.json --bail",
        "e2e:prod": "newman run e2e/collection.postman_collection.json -e e2e/prod.postman_environment.json --bail"
    },
    "devDependencies": {
        "@templatenametoreplace/example": "workspace:^0.1.0",
        "@types/aws-lambda": "^8.10.92",
        "@types/jest": "^28.1.7",
        "@types/webpack": "^5.28.0",
        "jest": "^28.1.3",
        "newman": "^5.3.2",
        "ts-jest": "^28.0.8",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.5.5",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    }
}
