{
    "name": "@templatenametoreplace/ui",
    "config": {
        "product": "templatenametoreplace",
        "component": "ui"
    },
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "@aws-amplify/auth": "^4.5.9",
        "@chakra-ui/react": "^2.2.4",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@templatenametoreplace/example": "workspace:^0.1.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "axios": "^0.27.2",
        "framer-motion": "^6.5.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.7.4",
        "web-vitals": "^2.1.4"
    },
    "overrides": {
        "react-refresh": "0.11.0"
    },
    "scripts": {
        "bootstrap": "./scripts/bootstrap.sh $npm_package_config_product $npm_package_config_component",
        "start": "DISABLE_ESLINT_PLUGIN=true craco start",
        "start:built": "serve -s build",
        "build": "DISABLE_ESLINT_PLUGIN=true craco build",
        "build:environment": "./scripts/build.sh",
        "deploy": "./scripts/deploy-amplify.sh $npm_package_config_product $npm_package_config_component",
        "test": "craco test --watchAll=false",
        "test:watch": "craco test",
        "test:withIntegration": "INCLUDE_INTEGRATION=true craco test --runInBand --watchAll=false",
        "test:e2e": "env-cmd -f .env.e2e npx playwright test",
        "test:e2e:manual": "npx playwright codegen",
        "e2e": "./scripts/e2e-test.sh",
        "storybook": "NODE_ENV=development start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@chakra-ui/storybook-addon": "^4.0.4",
        "@craco/craco": "^6.4.5",
        "@playwright/test": "^1.22.2",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-controls": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-interactions": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/builder-webpack5": "^6.5.9",
        "@storybook/manager-webpack5": "^6.5.9",
        "@storybook/node-logger": "^6.5.9",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.9",
        "@storybook/testing-library": "^0.0.13",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.8",
        "@types/testing-library__jest-dom": "^5.14.5",
        "babel-plugin-named-exports-order": "^0.0.2",
        "env-cmd": "^10.1.0",
        "prop-types": "^15.8.1",
        "react-app-alias": "^2.2.2",
        "serve": "^14.0.1",
        "ts-loader": "^9.4.1",
        "webpack": "^5.73.0"
    }
}